{
    "name": "astronphp/astronphp",
    "type": "library",
    "description": "Astronphp - Modular Framework for PHP",
    "keywords": ["framework php", "astronphp" , "php"],
    "license": "MIT",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/astronphp/astronphp/issues",
        "source": "https://github.com/astronphp/astronphp/"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "astronphp/framework": "0.0.*",
        "astronphp/frontview": "0.0.*",
        "astronphp/orm": "0.0.*",
        "astronphp/handler": "0.0.*"
    },
    "require-dev": {
        "astronphp/debugbar": "0.0.*"
    },
    "autoload": {
        "psr-4": {
            "Entity\\":  [
                "library/entity/"
            ],
            "Library\\":  [
                "library/src/",
                "library/tests/"
            ],
            "App\\": [
                "apps/"
            ]
        }
    }
}