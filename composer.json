{
    "name": "rocket-startup/rocket-startup",
    "type": "library",
    "description": "RocketStartup - Modular Framework for PHP 7",
    "keywords": ["framework php", "rocket-startup" , "php"],
    "license": "MIT",
    "minimum-stability": "alpha",
    "support": {
        "issues": "https://github.com/rocket-startup/rocket-startup/issues",
        "source": "https://github.com/rocket-startup/rocket-startup/"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7",
        "rocket-startup/framework": "0.0.*",
        "rocket-startup/frontview": "0.0.*",
        "rocket-startup/orm": "0.0.*"
    },
    "autoload": {
        "psr-4": {
            "Library\\":  [
                "library/src/",
                "library/tests/"
            ],
            "App\\": [
                "apps/"
            ]
        }
    }
}